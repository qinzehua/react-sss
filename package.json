{
  "name": "qzh_react_library",
  "version": "1.7.0",
  "private": false,
  "author": "qzh",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "description": "A React component library",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "build_library": "npm run clean && npm run build-ts && npm run build-css",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "dart-sass ./src/styles/index.scss ./dist/styles/index.css",
    "test": "react-scripts test",
    "test:noWatch": "react-scripts test --watchAll=false",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook dist",
    "prepublishOnly": "npm run lint && npm run test:noWatch && npm run build_library"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "async-validator": "^4.2.5",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "react-transition-group": "^4.4.5",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/preset-create-react-app": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-webpack5": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-transition-group": "^4.4.10",
    "dart-sass": "^1.25.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.5",
    "sass": "^1.74.1",
    "storybook": "^8.0.5",
    "typescript": "^4.9.5",
    "webpack": "^5.91.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:noWatch"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}